<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- [START] - Deploying Service Parameters -->
    <add key="ServiceInstaller.Name" value="WCF-API Server Provider"/>
    <add key="ServiceInstaller.Description" value="WCF-API Server Provider. Servicio proveedor/publicador de interfaces WCF."/>
    <!-- [END] - Deploying Service Parameters -->
    <!-- Directorio de despliegue de servicios (WCF)-->
    <add key="WCFServicesPath" value="d:\Services .NET\dotNET\WCF-API.Server\Services"/>
    <!-- Anyadir nombres de directorio separados por ";" -->
    <add key="LoadExceptions" value=""/>

    <!-- AÃ±adir servicio por KEY="nombre directorio" VALUE="nombre del ensamblado;clase a instanciar"-->
    <add key="Balancer.WCFService" value="DotNet.Balancer.WCFServices.dll;DotNet.Balancer.WCFServices.WCFServiceHostStarter"/>
  </appSettings>
<startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
</startup>
</configuration>
